      "flex-layout.col#cardCreative"
    ]
  },
  "flex-layout.col#cardCreative":{
    "children":[
      "rich-text#titleCreative",
      "rich-text#descriptionCreative"
    ]
  },

  // Seccion Explorar Productos
  "flex-layout.row#exploreProducts":{
    "children":[
      "flex-layout.col#exploreProducts"
    ]
  },
  "flex-layout.col#exploreProducts":{
    "children":[
      "flex-layout.row#exploreProductsNetwork",
      "flex-layout.row#exploreProductsButton"
    ]
  },
  "flex-layout.row#exploreProductsNetwork":{
    "children":[
      "flex-layout.col#exploreProductsNetwork_1",
      "flex-layout.col#exploreProductsNetwork_2"
    ],
    "props":{
      "preserveLayoutOnMobile": true
    }
  },
  "flex-layout.col#exploreProductsNetwork_1":{
    "props":{
      "blockClass":"exploreProductsNetwork_1"
    },
    "children":[
      "image#logoFacebookAB"
    ]
  },
  "flex-layout.col#exploreProductsNetwork_2":{
    "props":{
      "blockClass":"exploreProductsNetwork_2"
    },
    "children":[
      "image#logoInstagramAB"
    ]
  },
  "flex-layout.row#exploreProductsButton":{
    "children":[
      "flex-layout.col#exploreProductsButton"
    ]
  },
  "flex-layout.col#exploreProductsButton":{
    "props":{
      "blockClass":"exploreProductsButton"
    },
    "children":[
      "link#exploreProducts"
    ]
  },

