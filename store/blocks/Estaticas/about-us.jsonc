    "children":[
      "flex-layout.row#cardSmartContainerDesktop"
    ]
  },
  "flex-layout.row#cardSmartContainerDesktop":{
    "children":[
      "flex-layout.col#cardSmartDesktop_1",
      "flex-layout.col#cardSmartDesktop_2",
      "flex-layout.col#cardSmartDesktop_3"
    ]
  },
  "flex-layout.col#cardSmartDesktop_1":{
    "props":{
      "width":"33.333%",
      "blockClass": "cardSmartDesktop_1"
    },
    "children":[
      "flex-layout.row#bannerCardSmart_1",
      "rich-text#paragraphSmart_1"
    ]
  },
  "flex-layout.col#cardSmartDesktop_2":{
    "props":{
      "width":"33.333%",
      "blockClass": "cardSmartDesktop_2"
    },
    "children":[
      "flex-layout.row#bannerCardSmart_2",
      "rich-text#paragraphSmart_2"
    ]
  },
  "flex-layout.col#cardSmartDesktop_3":{
    "props":{
      "width":"33.333%",
      "blockClass": "cardSmartDesktop_3"
    },
    "children":[
      "flex-layout.row#bannerCardSmart_3",
      "rich-text#paragraphSmart_3"
    ]
  },
