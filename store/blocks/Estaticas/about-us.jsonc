{
  "store.custom#about-us": {
    "blocks": [
      "flex-layout.row#about-us"
    ]
  },

  // ========================================================== Maquetacion Basica
  "flex-layout.row#about-us": {
    "children": [
      "flex-layout.col#mediaQueries"
    ]
  },
  "flex-layout.col#mediaQueries": {
    "children": [
      "responsive-layout.desktop#maqDesktop",
      "responsive-layout.tablet#maqTablet",
      "responsive-layout.phone#maqPhone"
    ]
  },

  // ========================================================== Estructura por MediaQueries
  "responsive-layout.desktop#maqDesktop":{
    "children": [
      //"image#imgBanner",
      "flex-layout.row#bannerContent",
      "flex-layout.row#cardWelcomeDesktop",
      "flex-layout.row#separator1A",
      "flex-layout.row#cardSmartInfoDesktop",
      "flex-layout.row#separator2A",
      "flex-layout.row#cardCreativeDesktop",
      "flex-layout.row#exploreProducts"
    ]
  },
  "responsive-layout.tablet#maqTablet":{
    "children": [
      "flex-layout.row#bannerContent",
      "flex-layout.row#cardWelcomeTablet",
      "flex-layout.row#separator1A",
      "flex-layout.row#cardSmartInfoTablet",
      "flex-layout.row#separator2A",
      "flex-layout.row#cardCreativeTablet",
      "flex-layout.row#exploreProducts"
    ]
  },
  "responsive-layout.phone#maqPhone":{
    "children": [
      "flex-layout.row#bannerContentM",
      "flex-layout.row#cardWelcomeTablet",
      "flex-layout.row#separator1A",
      "flex-layout.row#cardSmartInfoTablet",
      "flex-layout.row#separator2A",
      "flex-layout.row#cardCreativeTablet",
      "flex-layout.row#exploreProducts"
    ]
  },

  // ========================================================== Estructura Escritorio
  //Banner
  "flex-layout.row#bannerContentM":{
    "children":[
      "flex-layout.col#bannerContentM"
    ]
  },
  "flex-layout.col#bannerContentM":{
    "props":{
      "blockClass":"bannerContent"
    },
    "children":[
      "image#imgBannerM",
      "flex-layout.row#bannerText"
    ]
  },

  "flex-layout.row#bannerContent":{
    "children":[
      "flex-layout.col#bannerContent"
    ]
  },
  "flex-layout.col#bannerContent":{
    "props":{
      "blockClass":"bannerContent"
    },
    "children":[
      "image#imgBanner",
      "flex-layout.row#bannerText"
    ]
  },
  "flex-layout.row#bannerText":{
    "props":{
      "blockClass":"bannerText"
    },
    "children":[
      "flex-layout.col#bannerText"
    ]
  },
  "flex-layout.col#bannerText":{
    "props":{
      "blockClass":"bannerText"
    },
    "children":[
      "rich-text#partnerIntegral",
      "rich-text#solucionesTecnologicas"
    ]
  },

  // Seccion Tarjeta de Bienvenida
  "flex-layout.row#cardWelcomeDesktop":{
    "props":{
      "blockClass":"cardWelcomeDesktop"
    },
    "children":[
      "flex-layout.col#cardWelcomeDesktop"
    ]
  },
  "flex-layout.col#cardWelcomeDesktop":{
    "props":{
      "blockClass":"cardWelcomeDesktop"
    },
    "children":[
      "rich-text#titleAboutUs",
      "rich-text#paragraph_01"
    ]
  },
  // Seccion Tarjeta Smart
  "flex-layout.row#cardSmartInfoDesktop":{
    "props":{
      "blockClass":"cardSmartInfoDesktop"
    },
    "children":[
      "flex-layout.col#cardSmartInfoDesktop"
    ]
  },
  "flex-layout.col#cardSmartInfoDesktop":{
    "props":{
      "blockClass":"cardSmartInfoDesktop"
